Algoritmo CrearSerieNatural1_N
	// (1) DEFINIR TIPO DATOS
	Definir n Como Entero
	// (2) ENTRADA
	Escribir "Ingrese n ? "
	Leer n //5
	// (3) PROCESO + SALIDA
	inicio = n
	Mientras inicio >= 1 Hacer
		Escribir inicio
		inicio = inicio - 1 //CONTADOR
	Fin Mientras
    Escribir "FIN"
FinAlgoritmo
